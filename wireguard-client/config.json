{
    "name": "Wireguard-Client",
    "version": "1.0.0",
    "slug": "wireguard-client",
    "description": "Wireguard vpn client for HASSIO",
    "url": "https://github.com/vaceslav/hassio-addons",
    "startup": "application",
    "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
