    "host_network": true,
    "hassio_api": true,
    "hassio_role": "default",
    "privileged": ["NET_ADMIN", "SYS_MODULE"],
    "devices": ["/dev/net/tun"],
    "map": ["config"],
    "kernel_modules": true,
    "options": {},
    "schema": {}
  }
